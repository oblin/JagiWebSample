
@{
    ViewBag.Title = "地址名稱維護";
    ViewBag.Name = "Others";
}

<h3>地址名稱維護</h3>

<div ng-controller="addressController as vm">
   <div ui-grid="vm.gridOptions" ui-grid-pagination style="height: 600px"></div>
</div>

@section Scripts{
<script src="~/app/adminAddress/addressController.js"></script>
    <script type="text/javascript">
        var getAddress = '@(Html.BuildUrlFromExpression<AddressController>(c => c.Get(null)))';

        window.app.constant("model", {
            'getAddressUrl': getAddress
        });
    </script>
}